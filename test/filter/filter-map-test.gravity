#include "./src/lo.gravity"

#unittest {
  name: "Lo.filter returns new map with elements passed predicate check";
  error: NONE;
  result: 2; // 2 pairs [1: "one", 4: "four"]
};

func main() {
  var map = [
    1: "one",
    "2": "two",
    [3]: ["three"],
    4: "four",
    ['5':[5]]: "five"
  ];

  return Lo.filter(map, func(value, key) {
    return key is Int && value is String;
  }).count;
}
